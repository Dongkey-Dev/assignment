{
  "name": "event-reward",
  "version": "0.0.1",
  "description": "",
  "author": "DongkeyDev",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:auth": "NODE_ENV=dev nest start auth",
    "start:auth:live": "NODE_ENV=live nest start auth",
    "start:auth:debug": "nest start auth --debug --watch",
    "start:gateway": "NODE_ENV=dev nest start gateway",
    "start:gateway:live": "NODE_ENV=live nest start gateway",
    "start:gateway:debug": "nest start gateway --debug --watch",
    "start:event": "NODE_ENV=dev nest start event",
    "start:event:live": "NODE_ENV=live nest start event",
    "start:event:debug": "nest start event --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit",
    "test:watch": "jest --watch --forceExit",
    "test:cov": "jest --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "prepare": "ts-patch install && typia patch",
    "swagger": "npx nestia swagger --config nestia.config.ts --config nestia-cli.config.json --project tsconfig.json",
    "sdk": "npx nestia sdk --config nestia.config.ts --project tsconfig.json",
    "start:swagger": "yarn swagger && ts-node executable/swagger.ts --skipBuild"
  },
  "dependencies": {
    "@nestia/core": "^3.2.5",
    "@nestia/e2e": "^0.6.0",
    "@nestia/fetcher": "^3.2.5",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.10",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.52.2",
    "install": "^0.13.0",
    "mongoose": "^8.15.0",
    "npm": "^10.8.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typia": "^6.2.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestia/sdk": "^3.2.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^10.0.0",
    "@swc-node/register": "^1.9.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.25",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.17",
    "@types/functional-red-black-tree": "^1.0.6",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nestia": "^5.3.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@auth(|/.*)$": "<rootDir>/apps/auth$1",
      "^@event(|/.*)$": "<rootDir>/apps/event$1",
      "^@gateway(|/.*)$": "<rootDir>/apps/gateway$1",
      "^@libs(|/.*)$": "<rootDir>/libs$1",
      "^@shared(|/.*)$": "<rootDir>/libs/shared$1"
    }
  },
  "resolutions": {
    "@nestjs/core": "10.4.17",
    "@nestjs/common": "10.4.17"
  }
}
